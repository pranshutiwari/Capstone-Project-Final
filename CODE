plot(ROCRperf4,colorize=TRUE,print.cutoffs.at=seq(0,1,0.1),text.adj=c(-0.2,1.8))
Trainlog5<-glm(Classification~BMI*Hmarkup,data=pima.indian.diabetestrain,family=binomial)
