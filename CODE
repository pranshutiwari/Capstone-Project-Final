Trainlog4<-glm(Classification~TricepsThickness*BMI*Age*Hmarkup*DiastolicBP,data=pima.indian.diabetestrain,family=binomial)
summary(Trainlog4)
predictTrain4<-predict(Trainlog4,type="response",data=pima.indian.diabetestrain)
